<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="eazylivings.com.eazylivings">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.RegisterNewUser" />
        <activity android:name=".activities.ForgotPassword" />
        <activity android:name=".activities.LoginPage" />
        <activity android:name=".activities.UserProfile"></activity>
        <activity android:name=".activities.Introduction"></activity>
    </application>

</manifest>